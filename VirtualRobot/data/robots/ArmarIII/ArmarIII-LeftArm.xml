		</CollisionModel>
		<Child name="Wrist 1 L"/>
	</RobotNode>
	
	<RobotNode name="Wrist 1 L">
		<Joint type="revolute">
			<DH a="0" d="0" theta="-90" alpha="-90" units="degree"/>
			<Limits unit="degree" lo="-29.36" hi="24.11"/>
		</Joint>
		<!--Physics>
            <CoM location="VisualizationBBoxCenter"/>
            <Mass value="1.29945309" units="kg" />
        </Physics-->
